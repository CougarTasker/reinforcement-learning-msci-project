{
    "grammarly.config.documentDialect": "british",
    "grammarly.config.documentDomain": "academic",
    "grammarly.files.include": [
        "**/readme.md",
        "**/README.md",
        "**/*.txt",
        "**/*.tex"
    ],
    "cSpell.language": "en-GB",
    "cSpell.words": [
        "backpropagation",
        "Kivy"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true
    }
}